{
	"OpenIdConnect": {
		"AuthorisationPath": "/connect/authorise",
		"LogoutPath": "/connect/logout",
		"TokenPath": "/connect/token",
		"UserInfoPath": "/connect/userinfo",
		"AllowInsecureHttp": "false",
		"EnableJWT": "false",
		"UseDeveloperKeys": "false"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Warning"
		}
	},
	"AllowedHosts": "*"
}
